set editing-mode emacs
set bell-style visible
set visible-stats on

"\C-x\C-r": re-read-init-file

"\C-d": backward-kill-word
"\C-g": kill-whole-line

# Jump words back/forward M-h/M-l
"\eh": backward-word
"\el": forward-word

# Completion
TAB: complete           # tab
"\e[Z": menu-complete   # shift-tab

"\eu": 'cd ..\015'      # Alt-u
